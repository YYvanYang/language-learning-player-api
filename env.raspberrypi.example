# .env file for docker-compose on Raspberry Pi

# PostgreSQL Credentials
POSTGRES_USER=your_db_user        # 修改成你想要的数据库用户名
POSTGRES_PASSWORD=your_strong_db_password # 修改成强密码
POSTGRES_DB=language_learner_db

# MinIO Credentials
MINIO_ACCESS_KEY=your_minio_access_key # 修改成你的 MinIO access key
MINIO_SECRET_KEY=your_strong_minio_secret_key # 修改成你的 MinIO secret key
MINIO_BUCKET_NAME=language-audio

# API JWT Secret (必须设置，且要足够复杂)
JWT_SECRETKEY=a_very_strong_and_long_secret_key_for_jwt_!@#$%^&*()_+

# Google OAuth (如果需要)
GOOGLE_CLIENTID=YOUR_GOOGLE_CLIENT_ID_HERE
GOOGLE_CLIENTSECRET=YOUR_GOOGLE_CLIENT_SECRET_HERE

# 其他可选配置 (如果需要覆盖 docker-compose.yml 中的默认值)
# SERVER_PORT=8080
# LOG_LEVEL=info
# LOG_JSON=true
# CORS_ALLOWEDORIGINS="http://your-frontend.com"